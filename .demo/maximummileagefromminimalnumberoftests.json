        {
          "action": "highlight",
          "path": ".demo/temp-files/1.cs",
          "position": "14,8:14,74",
          "highlightWholeLine": false
        }
      ]
    },
    {
      "title": "5-How-1-nice-framework-4",
      "steps": [
        {
          "action": "insert",
          "path": ".demo/temp-files/1.cs",
