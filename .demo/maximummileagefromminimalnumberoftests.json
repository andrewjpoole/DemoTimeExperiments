{
  "$schema": "https://demotime.show/demo-time.schema.json",
  "title": "MaximumMileageFromMinimalNumberOfTests",
  "version": 2,
  "demos": [
    {
      "id": "demo-mjyhvpx8-0o72",
      "title": "1 Initial Slides",
      "steps": [
        {
          "action": "openSlide",
          "path": ".demo/slides/initial-slides.md"
        }
      ]
    },
    {
      "id": "scene-mk481q72-osxg",
      "title": "Prepare vscode",
      "steps": [
        {
          "action": "zoomIn",
          "zoom": 3
        },
        {
          "action": "setSetting",
          "disabled": false,
          "setting": {
            "key": "editor.codeLens",
            "value": false
          }
        }
      ]
    },
    {
      "id": "demo-mjysqmwo-23e2",
      "title": "5-How-1-nice-framework-1",
      "steps": [
        {
          "action": "create",
          "path": ".demo/temp-files/1.cs",
          "contentPath": ".demo/code-snippets/component-test-class-blank.txt"
        },
        {
          "action": "open",
          "path": ".demo/temp-files/1.cs"
        }
      ]
    },
    {
      "title": "5-How-1-nice-framework-2",
      "steps": [
        {
          "action": "insert",
          "position": "11",
          "highlightWholeLine": true,
          "insertTypingMode": "character-by-character",
          "insertTypingSpeed": 10,
